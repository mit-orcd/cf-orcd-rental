# tests/system_tests/config/projects.yaml.example
# Example projects configuration for system tests
# Copy this file to projects.yaml and customize for your environment
version: "1.0"

projects:
  - id: project_alpha
    name: "Project Alpha"
    description: "Research project Alpha - demonstrates multiple members with mixed maintenance status"
    owner: user_pi_alpha
    members:
      - user_id: user_member_alpha1
        role: member
      - user_id: user_member_alpha2
        role: technical_admin

  - id: project_beta
    name: "Project Beta"
    description: "Research project Beta - demonstrates smaller team"
    owner: user_pi_beta
    members:
      - user_id: user_member_beta1
        role: technical_admin

  - id: project_gamma
    name: "Project Gamma"
    description: "Research project Gamma - single PI, no members"
    owner: user_pi_gamma
    members: []

  - id: project_delta
    name: "Project Delta"
    description: "Research project Delta - larger team with various roles"
    owner: user_pi_delta
    members:
      - user_id: user_member_delta1
        role: financial_admin
      - user_id: user_member_delta2
        role: member
      - user_id: user_member_delta3
        role: technical_admin
